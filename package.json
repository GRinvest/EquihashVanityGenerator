{
    "name": "equihashvanitygenerator",
    "version": "1.0.1",
    "description": "A vanity address generator for cryptocurrencies like ZClassic.",
    "keywords": [
        "vanity",
		"zcash",
		"zclassic",
        "equihash"
    ],
    "homepage": "https://github.com/GRinvest/EquihashVanityGenerator",
    "bugs": {
        "url": "https://github.com/GRinvest/EquihashVanityGenerator/issues"
    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/GRinvest/EquihashVanityGenerator.git"
    },
    "dependencies": {
        "node_modules/zcash-bitcore-lib": "git+https://github.com/bitmex/zcash-bitcore-lib.git"
    },
    "scripts": {
		"install": "LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$PWD/node_modules/zcash-bitcore-lib/",
         "start": "LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$PWD/node ./index.js"
    }
}